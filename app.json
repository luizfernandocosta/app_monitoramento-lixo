{
  "expo": {
    "name": "SignalBin",
    "slug": "SignalBin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tcc_icon.png",
    "splash": {
      "image": "./assets/tcc_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F8FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.signalbin.signalbin",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tcc_icon.png",
        "backgroundColor": "#F6F8FF"
      },
      "package": "com.signalbin.signalbin",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
